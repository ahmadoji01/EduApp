<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.educationalapp.eduapp.educationalapp.registermodule.Register1Fragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/register_start_string"
            android:gravity="center_vertical|center"/>

        <Button
            android:background="@color/colorButtonBlue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/start_string"
            android:layout_margin="10dp"
            android:textColor="@android:color/white"
            android:textSize="16dp"
            android:id="@+id/FragmentRegister_start_btn" />

    </LinearLayout>

</FrameLayout>
